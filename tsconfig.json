{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "@components": ["src/app/components"],
      "@images": ["src/images/*"]
    },
    "noImplicitAny": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6"
  },
  "include": [
    "./src/index.tsx"
  ]
}